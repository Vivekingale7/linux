/*
2 3 5
7 11 13
17 19 23
*/


#include<stdio.h>

void main(){

	int rows,x=2;

	printf("Enter Rows: ");
	scanf("%d",&rows);

	for(int i=1;i<=rows;i++){
	
		for(int j=1;j<=rows;){
		
			int cnt=0;
			int y=x;
			y--;
			//for(int k=1;k<x;k++){
			
				if(x%y==0){
				
					cnt++;
					
					if(y==0){
						
						break;
					}
					
				
				 y--;
				 continue;
				 //y--;
				}
			//}

			if(cnt==1){
			
			
				printf("%d\t",x);
				j++;
			}
			x++;
			}
		printf("\n");
		
	}

}
